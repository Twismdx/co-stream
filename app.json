{
  "expo": {
    "name": "co-stream",
    "jsEngine": "hermes",
    "slug": "co-stream",
    "version": "0.5.6",
    "orientation": "portrait",
    "icon": "./assets/costream_icon.png",
    "scheme": "com.costream",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splashscreen_image_foreground.png",
      "resizeMode": "contain",
      "backgroundColor": "#322257"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "app.co-stream.live",
              "pathPrefix": "/.well-known/assetlinks.json"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/costream_launch_foreground.png",
        "backgroundColor": "#322257"
      },
      "package": "com.costream",
      "permissions": [
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splashscreen_image_foreground.png",
          "resizeMode": "contain",
          "backgroundColor": "#322257"
        }
      ],
      [
        "./withNotifeeAndroidFix"
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "brellatech"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "2d06e151-fb4b-4e69-bcdf-93d7c390ec2b"
      }
    }
  }
}
