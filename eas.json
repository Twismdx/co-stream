{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": false,
      "env": { "EXPO_USE_COMMUNITY_AUTOLINKING": "1" }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": false,
      "env": { "EXPO_USE_COMMUNITY_AUTOLINKING": "1" }
    },
    "production": {
      "autoIncrement": false,
      "env": { "EXPO_USE_COMMUNITY_AUTOLINKING": "1" }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./service_account_key.json",
        "track": "internal"
      }
    }
  }
}
